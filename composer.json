{
	"name": "alleyinteractive/alley-coding-standards",
	"description": "PHPCS sniffs for Alley Interactive",
	"type": "phpcodesniffer-standard",
	"keywords": [ "phpcs", "static analysis" ],
	"license": "GPL-2.0-or-later",
	"require": {
		"automattic/vipwpcs": "^2.3.3",
		"phpcompatibility/phpcompatibility-wp": "^2.1.4",
		"wp-coding-standards/wpcs": "dev-2.3.0-php-8.1 as 2.3"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/alleyinteractive/WordPress-Coding-Standards"
		}
	]
}
